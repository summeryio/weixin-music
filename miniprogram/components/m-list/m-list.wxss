/* pages/playlist/playlist.wxss */

.m-list {
    padding-bottom: 40rpx;
}
.list {
    overflow: hidden;
}
.list .item {
    float: left;
    width: 50%;
    /* margin-bottom: 20rpx; */
    box-sizing: border-box;
}
.list .item:nth-child(odd) {
    padding-right: 8rpx;
}
.list .item navigator {
    width: 100%;
}
.list .img {
    width: 100%;
    height: 372rpx;
}
.list .desc {
    overflow: hidden;
    height: 80rpx;
    font-size: 28rpx;
}